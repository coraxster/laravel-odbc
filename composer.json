{
    "name": "coraxster/laravel-odbc",
    "description": "simple laravel odbc connector",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "coraxster",
            "email": "coraxster@gmail.com"
        }
    ],
    "require": {
        "illuminate/container": "5.*",
        "illuminate/database": "5.*",
        "ext-pdo": "*",
        "ext-pdo_odbc": "*"
    },
    "autoload": {
        "psr-0": {
            "Coraxster\\ODBCDriver": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Coraxster\\ODBCDriver\\ODBCServiceProvider"
            ]
        }
    }
}
